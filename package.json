{
  "name": "salesmaster",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm dotenv -e .env.local -- next dev -p 5000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "pnpm dotenv -e .env.local -- pnpm prisma db seed",
    "prisma-push-schema": "pnpm dotenv -e .env.local -- pnpm prisma db push && pnpm prisma generate",
    "prisma-pull-schema": "pnpm dotenv -e .env.local -- pnpm prisma db pull && pnpm prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compilerOptions {\"module\":\"CommonJS\"} ./src/prisma/seed/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@prisma/client": "^4.11.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "bcrypt": "^5.1.0",
    "cookie": "^0.5.0",
    "eslint": "8.34.0",
    "eslint-config-next": "^13.2.1",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.9.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "dotenv-cli": "^7.0.0",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1"
  }
}
